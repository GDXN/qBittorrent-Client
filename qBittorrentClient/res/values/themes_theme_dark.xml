<?xml version="1.0" encoding="utf-8"?>

<!-- Generated with http://android-holo-colors.com -->
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="Theme_Dark" parent="android:Theme.Holo">
        <!-- Window background -->
        <item name="android:windowBackground">@android:color/background_dark</item>

        <!-- dropshadow backgrouns (for torrent details)-->
        <item name="dropShadowBackground">@drawable/container_dropshadow_dark</item>

        <!-- backgroundSelector (for row listView)-->
        <item name="backgroundSelector">@drawable/background_selector_dark</item>


        <item name="android:editTextBackground">@drawable/theme_dark_edit_text_holo_dark</item>

        <item name="android:textColorHighlight">#992d2d2d</item>
        <item name="android:textSelectHandleLeft">@drawable/theme_dark_text_select_handle_left
        </item>
        <item name="android:textSelectHandleRight">@drawable/theme_dark_text_select_handle_right
        </item>
        <item name="android:textSelectHandle">@drawable/theme_dark_text_select_handle_middle</item>

        <item name="android:autoCompleteTextViewStyle">@style/AutoCompleteTextViewTheme_Dark</item>

        <item name="android:listChoiceIndicatorMultiple">@drawable/theme_dark_btn_check_holo_dark
        </item>

        <item name="android:listChoiceIndicatorSingle">@drawable/theme_dark_btn_radio_holo_dark
        </item>

        <item name="android:buttonStyle">@style/ButtonTheme_Dark</item>

        <item name="android:imageButtonStyle">@style/ImageButtonTheme_Dark</item>

        <item name="android:dropDownSpinnerStyle">@style/SpinnerTheme_Dark</item>

        <item name="android:progressBarStyleHorizontal">@style/ProgressBarTheme_Dark</item>

        <item name="android:seekBarStyle">@style/SeekBarTheme_Dark</item>

        <item name="android:ratingBarStyle">@style/RatingBarTheme_Dark</item>

        <item name="android:ratingBarStyleIndicator">@style/RatingBarBigTheme_Dark</item>

        <item name="android:ratingBarStyleSmall">@style/RatingBarSmallTheme_Dark</item>

        <item name="android:buttonStyleToggle">@style/ToggleTheme_Dark</item>

        <item name="android:listChoiceBackgroundIndicator">
            @drawable/theme_dark_list_selector_holo_dark
        </item>

        <item name="android:activatedBackgroundIndicator">
            @drawable/theme_dark_activated_background_holo_dark
        </item>

        <item name="android:fastScrollThumbDrawable">@drawable/theme_dark_fastscroll_thumb_holo
        </item>

        <!-- new -->
        <item name="android:actionBarItemBackground">@drawable/selectable_background_theme_dark
        </item>
        <item name="android:popupMenuStyle">@style/PopupMenu.Theme_Dark</item>
        <item name="android:dropDownListViewStyle">@style/DropDownListView.Theme_Dark</item>
        <item name="android:actionBarTabStyle">@style/ActionBarTabStyle.Theme_Dark</item>
        <item name="android:actionDropDownStyle">@style/DropDownNav.Theme_Dark</item>
        <item name="android:actionBarStyle">@style/ActionBar.Transparent.Theme_Dark</item>
        <item name="android:actionModeBackground">@drawable/cab_background_top_theme_dark</item>
        <item name="android:actionModeSplitBackground">@drawable/cab_background_bottom_theme_dark
        </item>
        <item name="android:actionModeCloseButtonStyle">@style/ActionButton.CloseMode.Theme_Dark
        </item>

    </style>


    <style name="ActionBar.Solid.Theme_Dark" parent="@android:style/Widget.Holo.ActionBar.Solid">
        <item name="android:background">@drawable/ab_solid_theme_dark</item>
        <item name="android:backgroundStacked">@drawable/ab_stacked_solid_theme_dark</item>
        <item name="android:backgroundSplit">@drawable/ab_bottom_solid_theme_dark</item>
        <item name="android:progressBarStyle">@style/ProgressBar.Theme_Dark</item>
    </style>

    <style name="ActionBar.Transparent.Theme_Dark" parent="@android:style/Widget.Holo.ActionBar">
        <item name="android:background">@drawable/ab_transparent_theme_dark</item>
        <item name="android:progressBarStyle">@style/ProgressBar.Theme_Dark</item>
    </style>

    <style name="PopupMenu.Theme_Dark" parent="@android:style/Widget.Holo.ListPopupWindow">
        <item name="android:popupBackground">@drawable/menu_dropdown_panel_theme_dark</item>
    </style>

    <style name="DropDownListView.Theme_Dark" parent="@android:style/Widget.Holo.ListView.DropDown">
        <item name="android:listSelector">@drawable/selectable_background_theme_dark</item>
    </style>

    <style name="ActionBarTabStyle.Theme_Dark" parent="@android:style/Widget.Holo.ActionBar.TabView">
        <item name="android:background">@drawable/tab_indicator_ab_theme_dark</item>
    </style>

    <style name="DropDownNav.Theme_Dark" parent="@android:style/Widget.Holo.Spinner">
        <item name="android:background">@drawable/spinner_background_ab_theme_dark</item>
        <item name="android:popupBackground">@drawable/menu_dropdown_panel_theme_dark</item>
        <item name="android:dropDownSelector">@drawable/selectable_background_theme_dark</item>
    </style>

    <style name="ProgressBar.Theme_Dark" parent="@android:style/Widget.Holo.ProgressBar.Horizontal">
        <item name="android:progressDrawable">@drawable/progress_horizontal_theme_dark</item>
    </style>

    <style name="ActionButton.CloseMode.Theme_Dark" parent="@android:style/Widget.Holo.ActionButton.CloseMode">
        <item name="android:background">@drawable/btn_cab_done_theme_dark</item>
    </style>

    <!-- this style is only referenced in a Light.DarkActionBar based theme -->
    <style name="Theme.Theme_Dark.Widget" parent="@android:style/Theme.Holo">
        <item name="android:popupMenuStyle">@style/PopupMenu.Theme_Dark</item>
        <item name="android:dropDownListViewStyle">@style/DropDownListView.Theme_Dark</item>
    </style>

</resources>
